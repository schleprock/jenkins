    <hudson.tasks.Shell>
      <command>if [ &quot;${VERSION}&quot; == &quot;needOne&quot; ]; then
  echo &quot;ERROR: need a version, current is ${VERSION}&quot;
  exit 3
fi

echo &quot;testing ${VERSION}&quot;
pwd
currDir=`pwd`

testdir=&quot;${currDir}/simplorer/Tests/New_VHDL/testcases/compile_only&quot;
if ! pushd $testdir &gt; /dev/null; then
   echo &quot;ERROR: unable to cd to $testdir&quot;
   exit 1
fi

perlCmd=&quot;./runTests.pl --type $VERSION --noxmessage&quot;
echo &quot;Run tests: $perlCmd&quot;
if ! $perlCmd; then
   echo &quot;...FAILURE...&quot;
   echo &quot;Failed test list:&quot;
   cat failmodels.txt
   echo
   exit 1
fi
echo &quot;Test completed successfully&quot;
exit 0
</command>
    </hudson.tasks.Shell>
